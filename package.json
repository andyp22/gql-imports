  "engines": {
    "node": ">=4.1.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-string": "^1.2.0",
    "del-cli": "^0.2.0",
    "ghooks": "^1.2.4",
    "mocha": "^2.5.3",
    "tslint": "^3.12.1",
    "tslint-microsoft-contrib": "^2.0.8",
    "typescript": "^1.8.10",
    "typings": "^1.3.1"
